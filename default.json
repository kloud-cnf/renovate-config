{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "kolvin"
  ],
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    ":ignoreUnstable",
    ":rebaseStalePrs",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "labels": [
    "dependencies"
  ],
  "schedule": [
    "after 10pm every weekday",
    "before 5am every weekday",
    "every weekend"
  ]
}
